    <h4 class="modal-title" id="myModalLabel">Are you sure to delete?</h4>
</div>
<div class="modal-body">


    <dl class="dl-horizontal">


        <dt>
            @Html.DisplayNameFor(model => model.issuesdescription)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.issuesdescription)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.dateencoded)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.dateencoded)
        </dd>

        

       
